<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="FilrougeEntities" transaction-type="JTA">
<!-- 		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider> -->
	<non-jta-data-source>java:/filrouge</non-jta-data-source>
		<class>model.ActionANotifier</class>
		<class>model.Candidature</class>
		<class>model.Competence</class>
		<class>model.Droit</class>
		<class>model.Envoyer</class>
		<class>model.EnvoyerPK</class>
		<class>model.InvitationProjet</class>
		<class>model.Possede</class>
		<class>model.PossedePK</class>
		<class>model.Profil</class>
		<class>model.Projet</class>
		<class>model.Promotion</class>
		<class>model.PropositionComp</class>
		<class>model.Travaille</class>
		<class>model.TravaillePK</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/filrouge"/>
			<property name="javax.persistence.jdbc.user" value="postgres"/>
			<property name="javax.persistence.jdbc.password" value="postgres"/>
			<property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
		</properties>
	</persistence-unit>
	</persistence>